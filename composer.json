{
    "name": "monomelodies/monolyth",
    "description": "Object oriented PHP5 MVC unframework",
    "keywords": ["PHP","OO","MVC","framework","unframework"],
    "license": "MIT",
    "authors": [
        {
            "name": "Marijn Ophorst",
            "email": "marijn@monomelodies.nl"
        }
    ],
    "require": {
        "php": ">=5.4",
        "twig/twig": "^1.0.0",
        "monomelodies/disclosure": "^1.0.0",
        "monomelodies/improse": "^0.3.0",
        "monomelodies/reroute": "^2.0.0",
        "monomelodies/cesession": "^0.0.3",
        "monomelodies/emily": "^1.1.5",
        "monomelodies/booby": "^1.1.1",
        "monomelodies/envy": "^0.2.0",
        "monomelodies/formulaic": "^1.4.3",
        "monomelodies/gentry": "^0.3.0",
        "monomelodies/metaculous": "^1.0.1",
        "monomelodies/monki": "^0.1.4",
        "monomelodies/mural": "^0.2.0",
        "monomelodies/dormant": "^0.0.4"
    },
    "require-dev": {
        "phpunit/phpunit": ">=4"
    },
    "suggest": {
        "phpunit/dbunit": "Test your database classes too",
        "twig/extensions": "Handy Twig extensions (e.g. for i18n)"
    },
    "autoload": {
        "psr-4": {
            "": "src/"
        }
    }
}
