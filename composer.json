{
    "name": "monolyth/frontal",
    "description": "Monolyth unframework Front controller package",
    "keywords": ["PHP","OO","MVC","framework","unframework","front controller"],
    "license": "MIT",
    "authors": [
        {
            "name": "Marijn Ophorst",
            "email": "marijn@monomelodies.nl"
        }
    ],
    "require": {
        "php": ">=5.4",
        "league/pipeline": "^0.1.0",
        "zendframework/zend-diactoros": "^1.2"
    },
    "require-dev": {
        "gentry/gentry": "^0.10.0"
    },
    "autoload": {
        "psr-4": {
            "Monolyth\\Frontal\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "": "example/src",
            "Monolyth\\Frontal\\": "src/",
            "Monolyth\\Frontal\\Test\\": "tests/gentry/"
        },
        "files": [
            "tests/HeaderStack.php"
        ]
    },
    "suggest": {
        "filp/whoops": "PHP errors for cool kids"
    }
}
