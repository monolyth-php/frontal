{
    "name": "monolyth/frontal",
    "description": "Monolyth unframework Front controller package",
    "keywords": ["PHP","OO","MVC","framework","unframework","front controller"],
    "license": "MIT",
    "authors": [
        {
            "name": "Marijn Ophorst",
            "email": "marijn@monomelodies.nl"
        }
    ],
    "require": {
        "php": ">=7.3",
        "league/pipeline": "^0.1.0",
        "laminas/laminas-diactoros": "^2.2",
        "laminas/laminas-httphandlerrunner": "^2.5"
    },
    "require-dev": {
        "toast/unit": "^2.0",
        "gentry/gentry": "^0.13.12",
        "gentry/toast": "^0.1.1"
    },
    "autoload": {
        "psr-4": {
            "Monolyth\\Frontal\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Monolyth\\Frontal\\": "src/"
        }
    },
    "suggest": {
        "monolyth/plumber": "Monolyth utilities",
        "filp/whoops": "PHP errors for cool kids",
        "twig/twig": "Twig templating engine"
    }
}
